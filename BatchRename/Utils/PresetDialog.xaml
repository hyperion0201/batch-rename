<Window x:Class="BatchRename.Utils.PresetDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename.Utils"
        mc:Ignorable="d"
        Title="Save Preset" Height="250" Width="300">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center">
        <StackPanel Margin="0,0,0,30" Orientation="Horizontal" HorizontalAlignment="Center">
            <TextBlock Text="Save as : "></TextBlock>
            <TextBox Name="presetTextBox" Width="100"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Width="50"  Margin="0,0,30,0" Click="OnSave" Content="Save"/>
            <Button Width="50" Click="Cancel" Content="Cancel"/>
        </StackPanel>
       
    </StackPanel>
</Window>
